{
  "name": "redis-analytics",
  "version": "0.1.0",
  "description": "Reusable Redis analytics primitives for TS/HLL/Bloom-backed metrics.",
  "type": "module",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattcattb/redis-analytics.git"
  },
  "homepage": "https://mattcattb.github.io/redis-analytics/",
  "bugs": {
    "url": "https://github.com/mattcattb/redis-analytics/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./bootstrap": {
      "types": "./dist/bootstrap.d.ts",
      "import": "./dist/bootstrap.js"
    },
    "./metric-registry": {
      "types": "./dist/metric-registry.d.ts",
      "import": "./dist/metric-registry.js"
    },
    "./time": {
      "types": "./dist/time/index.d.ts",
      "import": "./dist/time/index.js"
    },
    "./redis/bloom": {
      "types": "./dist/redis/bloom.service.d.ts",
      "import": "./dist/redis/bloom.service.js"
    },
    "./redis/hll": {
      "types": "./dist/redis/hll.service.d.ts",
      "import": "./dist/redis/hll.service.js"
    },
    "./redis/timeseries": {
      "types": "./dist/redis/timeseries.service.d.ts",
      "import": "./dist/redis/timeseries.service.js"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "import": "./dist/query/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc --build",
    "test": "vitest run",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepack": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "redis": "^4.7.0",
    "typescript": "^5.8.2",
    "vitepress": "^1.6.4",
    "vitest": "^2.1.8"
  }
}
