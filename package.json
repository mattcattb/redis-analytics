{
  "name": "redis-analytics",
  "version": "0.1.0",
  "description": "Reusable Redis analytics primitives for TS/HLL/Bloom-backed metrics.",
  "type": "module",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattcattb/redis-analytics.git"
  },
  "homepage": "https://mattcattb.github.io/redis-analytics/",
  "bugs": {
    "url": "https://github.com/mattcattb/redis-analytics/issues"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./types": "./src/types.ts",
    "./bootstrap": "./src/bootstrap.ts",
    "./metric-registry": "./src/metric-registry.ts",
    "./time": "./src/time/index.ts",
    "./redis/bloom": "./src/redis/bloom.service.ts",
    "./redis/hll": "./src/redis/hll.service.ts",
    "./redis/timeseries": "./src/redis/timeseries.service.ts",
    "./store": "./src/store/index.ts",
    "./query": "./src/query/index.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc --build",
    "test": "vitest run",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "redis": "^4.7.0",
    "typescript": "^5.8.2",
    "vitepress": "^1.6.4",
    "vitest": "^2.1.8"
  }
}
